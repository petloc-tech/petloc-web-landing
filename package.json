{
  "name": "petloc-web-landing",
  "packageManager": "yarn@3.6.4",
  "version": "0.1.0",
  "author": {
    "name": "Petloc"
  },
  "scripts": {
    "watch": "tailwindcss -i ./styles.css -o ./src/css/styles.css --watch",
    "copy": "copyfiles ./src/**/* ./public",
    "build": "tailwindcss -i ./styles.css -o ./src/css/styles.css --minify && npm run copy"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "copyfiles": "^2.4.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  }
}
